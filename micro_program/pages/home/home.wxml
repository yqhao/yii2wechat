<view class="app">
  <template name="zcCarousel">
    <swiper indicator-dots="true" circular="true" autoplay="{{data.customFeature.autoplay}}" vertical="{{data.customFeature.vertical}}" interval="{{data.customFeature.interval*1000}}" duration="750" style="{{data.style}}">
      <block wx:for="{{data.content}}">
        <swiper-item data-event-params="{{item.eventParams}}" catchtap="{{item.eventHandler}}">
          <image class="swiper-item" src="{{item.pic}}" />
        </swiper-item>
      </block>
    </swiper>
  </template>
  <view class="ele-container">
    <template is="zcCarousel" data="{{data:carousel1}}"></template>
  </view>

  <!-- 悬浮按钮 -->
  <template name="zcSuspension3">
    <view class="element suspension" data-compid="suspension3" style="{{data.style}}bottom: {{data.suspension_bottom}}rpx;">
      <view class="" wx:for="{{data.content}}" data-event-params="{{item.eventParams}}" catchtap="{{item.eventHandler}}">
        <view class="suspension-item diy" style="{{data.list_style}}">
          <image style="width:{{item.customFeature.width}};height:{{item.customFeature.width}}" src="{{item.pic}}"></image>
        </view>
      </view>
      <view>
        <view class="suspension-item" data-type="service" wx:if="{{data.customFeature.appendComponent.service}}" style="{{data.list_style}}">
          <icon class="ico-moon icon-service2 suspension-service">
            <contact-button type="default-dark" size="20" session-from="suspension"></contact-button>
          </icon>
        </view>
        <view class="suspension-item" data-router="myOrder" wx:if="{{data.customFeature.appendComponent.myOrder}}" catchtap="UserCenterTurnToPage" style="{{data.list_style}}">
          <icon class="ico-moon icon-notebook"></icon>
        </view>
        <view class="suspension-item" data-router="shoppingCart" wx:if="{{data.customFeature.appendComponent.shoppingCart}}" catchtap="UserCenterTurnToPage" style="{{data.list_style}}">
          <icon class="ico-moon icon-shoppingcart"></icon>
        </view>
        <view class="suspension-item" data-type="top" wx:if="{{false}}" style="{{data.list_style}}">
          <icon class="ico-moon icon-top"></icon>
        </view>
      </view>
    </view>
  </template>
  <template is="zcSuspension3" data="{{data:suspension3}}"></template>
  <!-- 悬浮按钮结束 -->

  <!-- 排序 -->
  <template name="zcSort">
    <view class="element sort" style="{{data.style}}">
      <scroll-view scroll-x="true" data-selected="{{data.customFeature.selected}}" style="{{data.ul_style}}">
        <block wx:for="{{data.content}}" wx:for-index="idx" wx:for-item="itemName">
          <block wx:if="{{idx == data.customFeature.selected}}">
            <view class="item refresh selected" catchtap="sortListFunc" data-compid="{{ data.compId }}" data-listid="{{data.customFeature.sort_object}}" data-sortkey="{{itemName.customFeature.sort_key}}" data-sortdirection="{{itemName.customFeature.sort_direction}}"
              data-idx="{{idx}}" style="color:{{data.customFeature.selectedColor}}">
              <text>{{itemName.text}}</text>
              <label class="arr-wrap" wx:if="{{ idx != 0 }}">
                <text style="color: {{itemName.customFeature.sort_direction == 0 ? data.customFeature.selectedColor : '#ddd'}};" class="sort-arr arr-up">▲</text>
                <text style="color: {{itemName.customFeature.sort_direction == 1 ? data.customFeature.selectedColor : '#ddd'}};" class="sort-arr arr-down">▼</text>
              </label>
            </view>
          </block>
          <block wx:else>
            <view class="item refresh" catchtap="sortListFunc" data-compid="{{ data.compId }}" data-listid="{{data.customFeature.sort_object}}" data-sortkey="{{itemName.customFeature.sort_key}}" data-sortdirection="{{itemName.customFeature.sort_direction}}" data-idx="{{idx}}">{{itemName.text}}</view>
          </block>
        </block>
      </scroll-view>
    </view>
  </template>
  <view class="ele-container">
    <template is="zcSort" data="{{data:sort5}}"></template>
  </view>
  <!-- 排序结束 -->
