.communityPage-wrap{
  font-size: 14px;
}
.community-li-cover{
  display: inline-block;
  width: 40px;
  height: 40px;
  border-radius: 20px;
  border: 1px solid #eee;
  overflow: hidden;
  vertical-align: bottom;
}
.community-li-cover > image{
  width: 100%;
  height: 100%;
}
.community-li-author{
  display: inline-block;
  margin-left: 10px;
  vertical-align: bottom;
}
.community-li-name{
  display: block;
  color: #f2b302;
  margin-bottom: 5px;
}
.community-li-time{
  display: block;
  font-size: 12px;
  color: #959595;
}
.community-hot-wrap{
  display: inline-block;
  color: #ff6c3e;
  font-size: 12px;
  float: right;
}
.community-hot-wrap > text{
  vertical-align: middle;
}
.community-hot{
  display: inline-block;
  width: 16px;
  height: 16px;
  background-color: #ff6c3e;
  font-size: 12px;
  color: #fff;
  text-align: center;
  line-height: 16px;
  margin-right: 5px;
  vertical-align: middle;
}
.community-hot:before{
  display: block;
}





.communityPage-wrap{
  background-color: #eee;
  height: 100vh;
}
.communityPage-head{
  padding: 10px 15px;
  background-color: #fff;
}
.communityPage-search{
  display: inline-block;
  width: 503.1rpx;
  height: 30px;
  border-radius: 4px;
  border: 1px solid #d4d4d4;
  vertical-align: top;
}
.communityPage-search-input{
  display: inline-block;
  width: 409.5rpx;
  padding: 0 11.7rpx;
  height: 30px;
  border: none;
}
.communityPage-search-btn{
  display: inline-block;
  width: 70.2rpx;
  height: 30px;
  color: #b2b2b2;
  text-align: center;
  vertical-align: top;
}
.communityPage-search-btn > icon{
  font-size: 16px;
  line-height: 30px;
}
.communityPage-topic-btn ,
.communityPage-notify-btn{
  display: inline-block;
  width: 70rpx;
  height: 30px;
  vertical-align: top;
  text-align: center;
  line-height: 30px;
  font-size: 20px;
  color: #00b6f8;
  margin-left: 18rpx;
  vertical-align: top;
}
.communityPage-carousel{
  width: 100%;
  height: 375rpx;
  /*display: none;*/
}
.communityPage-carousel image{
  width: 100%;
  height: 100%;
}
.communityPage-main{
  background-color: #fff;
}
.communityPage-info{
  padding: 10px 0;
}
.communityPage-img{
  width: 60px;
  height: 60px;
  margin-left: 15px;
  vertical-align: bottom;
}
.communityPage-info > view{
  display: inline-block;
  margin-left: 10px;
  vertical-align: bottom;
}
.communityPage-title{
  display: block;
  font-size: 16px;
  color: #0e0e0e;
  margin-bottom: 10px;
}
.communityPage-num{
  font-size: 14px;
}
.communityPage-num > text{
  color: #a3a3a3;
  margin-right: 20px;
}
.communityPage-num .communityPage-topic-num,
.communityPage-num .communityPage-comment-num{
  color: #00b6f8;
  margin-left: 5px;
}
.communityPage-topic{
  border-top: 1px solid #f3f3f3;
  height: 41px;
  white-space: nowrap;
  overflow: auto;
}
.communityPage-topic .navli{
  display: inline-block;
  width: 65px;
  color: #a3a3a3;
  text-align: center;
  line-height: 40px;
  overflow: hidden;
}
.communityPage-topic .navli.active{
  color: #00b6f8;
  border-bottom: 1px solid #00b6f8;
}
.communityPage-list{
  overflow: hidden;
}
.communityPage-li{
  margin-top: 10px;
  background-color: #fff;
  padding: 15px;
}
.communityPage-li-title{
  display: block;
  margin-top: 10px;
  font-size: 16px;
  color: #313131;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}
.communityPage-li-content{
  display: block;
  font-size: 12px;
  color: #909090;
  margin-top: 10px;
  overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 5;
  -webkit-box-orient: vertical;
}
.text-p{
  display: block;
}
.communityPage-li-info{
  float: right;
  color: #a3a3a3;
  margin-left: 10px;
  position: relative;
}
.communityPage-li-imgli-wrap{
  margin-top: 10px;
}
.communityPage-li-imgli{
  display: inline-block;
  width: 210.6rpx;
  height: 210.6rpx;
  margin: 7.02rpx;
  overflow: hidden;
  vertical-align: top;
}
.communityPage-li-imgli-one{
  margin: 7.02rpx;
}
.communityPage-li-imgli > image{
  width: 100%;
  height: 100%;
}
.communityPage-li-info > icon{
  font-size: 14px;
  margin-right: 3px;
  vertical-align: middle;
}
.communityPage-li-info > icon:before{
  display: block;
}
.communityPage-li-info > text{
  vertical-align: middle;
}
.communityPage-li-foot{
  margin-top: 10px;
  height: 20px;
}
.communityPage-li-none{
  text-align: center;
  margin-top: 20px;
}
.communityPage-li-none image{
  display: block;
  margin: 20px auto;
  width: 170px;
  height: 88px;
}
.communityPage-li-nomore{
  padding: 15px 0;
  text-align: center;
  font-size: 12px;
  color: #949494;
}
.ellipsis-a{
  color: #00b6f8;
  margin-left: 8px;
}
.communityPage-publish-btn{
  width: 105.3rpx;
  height: 105.3rpx;
  border-radius: 50%;
  background-color: #00b6f8;
  color: #fff;
  text-align: center;
  font-size: 28.08rpx;
  line-height: 105.3rpx;
  box-shadow: 0px 2px 6px #949494;
  position: fixed;
  bottom: 30px;
  right: 15px;
  transition: bottom 0.5s ease;
  -moz-transition: bottom 0.5s ease; 
  -webkit-transition: bottom 0.5s ease;
  -o-transition: bottom 0.5s ease;
}
.communityPage-publish-btn.bottom{
  bottom: -105.3rpx;
}
